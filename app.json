{
  "expo": {
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to process text from them."
        }
      ],
      [
        "@react-native-voice/voice",
        {
          "microphonePermission": "Allow CloudNotes to access the microphone",
          "speechRecognitionPermission": "Allow CloudNotes to securely recognize user speech"
        }
      ],
      [
        "expo-av",
        {
          "microphonePermission": "Allow CloudNotes to access your microphone."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow CloudNotes to access your photos.",
          "savePhotosPermission": "Allow CloudNotes to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "@react-native-google-signin/google-signin"
      ]
    ],
    "name": "Cloud-Notes",
    "slug": "cloud-notes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/favicon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/favicon.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.pankajisrani.cloudnotes"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "02744537-0b50-4bae-ad1a-fa776a4a5125"
      }
    },
    "owner": "slayersoull12",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/02744537-0b50-4bae-ad1a-fa776a4a5125"
    }
  }
}